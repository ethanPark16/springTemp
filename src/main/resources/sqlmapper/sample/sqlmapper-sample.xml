<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="sample">

    <select id="selectUser"  resultType="java.util.HashMap">
        select user_id, user_name from mobility.user_info_mst where user_id = #{userId}
    </select>
    
    <select id="selectUserList" resultType="com.pidotech.mobility.sample.service.SampleVO">
        select user_id, user_name from mobility.user_info_mst
    </select>
</mapper>
